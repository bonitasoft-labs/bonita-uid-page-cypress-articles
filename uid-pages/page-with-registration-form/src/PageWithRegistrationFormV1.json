    },
    "ctrl" : {
      "type" : "expression",
      "value" : [
        "return {",
        "    // Function that will check if the passwords ",
        "    // don't match and do the API call if they do",
        "    makeAPICallIfPasswordsMatch: function() {",
        "        // begin user creation",
        "        $data.creatingUser = true;",
        "        if ($data.formValues.password === $data.formValues.password_confirm) {",
        "            return \"../API/identity/user/\";",
        "        }",
        "        $data.errorMessage = uiTranslate(\"Passwords don't match.\");",
        "        $data.creatingUser = false;",
        "        return \"\";",
        "    },",
        "    isErrorOfType: function (type) {",
        "        if ($data.failedCreateResponse ",
        "            && $data.failedCreateResponse.cause",
        "            && ($data.failedCreateResponse.cause.exception.indexOf(type) > -1",
        "                || $data.failedCreateResponse.cause.message.indexOf(type) > -1)) {",
        "            return true;",
        "        }",
        "        return false;",
        "    }",
        "};"
      ],
      "exposed" : false
    },
    "errorMessage" : {
      "type" : "constant",
      "exposed" : false
    },
    "eventHandler" : {
      "type" : "expression",
      "value" : [
        "// Make error message disappear after 3 seconds, when user modifies the form",
        "if ($data.errorMessage !== \"\") {",
        "    setTimeout(function() {",
        "        $data.errorMessage = \"\";",
        "    }, 3000);",
        "}",
        "",
        "// Display any errors that might have happened",
        "if($data.createUserResponseStatusCode && $data.createUserResponseStatusCode !== \"\") {",
        "    switch($data.createUserResponseStatusCode) {",
        "        case 401:",
        "            $data.errorMessage = uiTranslate(\"Access denied.\");",
        "            break;",
        "        case 403:",
        "            $data.errorMessage = uiTranslate(\"The user already exists.\");",
        "            break;",
        "        case 500:",
        "            $data.errorMessage = uiTranslate(\"An error has occurred.\");",
        "            break;",
        "    }",
        "    // End user creation",
        "    $data.creatingUser = false;",
        "    // Erase message after 3 seconds, when user modifies the form",
        "    setTimeout(function() {",
        "        $data.createUserResponseStatusCode = \"\";",
        "    }, 3000);",
        "}"
      ],
      "exposed" : false
    },
    "creatingUser" : {
      "type" : "expression",
      "value" : [
        "return false;"
      ],
